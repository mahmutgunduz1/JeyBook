<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/nav_grapth"
    app:startDestination="@id/discoverFragment">

    <fragment
        android:id="@+id/discoverFragment"
        android:name="com.mahmutgunduz.jeybook.Fragment.BottomNavFragment.DiscoverFragment"
        android:label="fragment_kesvet"
        tools:layout="@layout/fragment_discover" />
    <fragment
        android:id="@+id/searchFragment"
        android:name="com.mahmutgunduz.jeybook.Fragment.BottomNavFragment.SearchFragment"
        android:label="fragment_search"
        tools:layout="@layout/fragment_search" >
        <action
            android:id="@+id/action_searchFragment_to_designFragment"
            app:destination="@id/designFragment" />
    </fragment>
    <fragment
        android:id="@+id/saveFragment"
        android:name="com.mahmutgunduz.jeybook.Fragment.BottomNavFragment.SaveFragment"
        android:label="fragment_save"
        tools:layout="@layout/fragment_save" />
    <fragment
        android:id="@+id/profileFragment"
        android:name="com.mahmutgunduz.jeybook.Fragment.ProfileFragment"
        android:label="fragment_profile"
        tools:layout="@layout/fragment_profile" />
    <fragment
        android:id="@+id/homePageFragment"
        android:name="com.mahmutgunduz.jeybook.Fragment.BottomNavFragment.HomePageFragment"
        android:label="fragment_home_page"
        tools:layout="@layout/fragment_home_page" />
    <fragment
        android:id="@+id/designFragment"
        android:name="com.mahmutgunduz.jeybook.Fragment.BottomNavFragment.DesignFragment"
        android:label="fragment_design"
        tools:layout="@layout/fragment_design" />
</navigation>